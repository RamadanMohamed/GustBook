@model IEnumerable<Guestbook.Models.AddSms>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ContentMs)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ContentMs)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.IdMs }, new { @class = "btn btn-warning" }) | |
                @Html.ActionLink("Details", "Details", new { id = item.IdMs }, new { @class = "btn btn-info" }) | |
                @Html.ActionLink("Delete", "Delete", new { id = item.IdMs }, new { @class = "btn btn-danger" }) |
            </td>
        </tr>
    }

</table>
<div>
    @Html.ActionLink("Continue", "SendMail", null, new { @class = "btn btn-danger", href = "/Sendmail/Index" })
</div>
